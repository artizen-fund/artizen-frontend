{
  "name": "artizen-frontend",
  "version": "0.1.26",
  "private": true,
  "engines": {
    "node": ">=16.14.0 <17",
    "yarn": "^1.22.10"
  },
  "overrides": {
    "webpack": "^5"
  },
  "scripts": {
    "clean": "rm -rf .next",
    "generate": "node ./tools/generate.js",
    "dev": "SECRET_ENV=DevSecrets op run --no-masking --env-file='.env.local' -- next dev",
    "develop": "yarn codegen-local && next dev",
    "build": "yarn codegen && next build",
    "start": "next build && next start -p $PORT",
    "lint": "next lint",
    "test:jest": "jest --watch",
    "test:quick": "start-server-and-test 'yarn dev' http-get://localhost:3000 'env-cmd -f .env.development npx synpress run -cf synpress.config.js'",
    "test:e2e": "PORT=3000 start-server-and-test 'yarn start' http-get://localhost:3000 'env-cmd -f .env.development synpress run -cf synpress.config.js'",
    "codegen": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config ./tools/graphql-codegen.yml -r dotenv/config",
    "codegen-local": "DOTENV_CONFIG_PATH=./.env.local SECRET_ENV=DevSecrets op run --env-file='.env.local' -- graphql-codegen --config ./tools/graphql-codegen.yml -r dotenv/config",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
